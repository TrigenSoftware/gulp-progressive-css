{
  "name": "gulp-progressive-css",
  "version": "1.3.0",
  "description": "Transfrom `<link>` in HTML to progressive CSS loading.",
  "main": "lib/index.js",
  "scripts": {
    "eslint": "eslint --cache 'src/*.js'",
    "compile": "npm run eslint && babel ./src -d ./lib",
    "prepublish": "npm run eslint && npm run compile"
  },
  "repository": "https://github.com/TrigenSoftware/gulp-progressive-css",
  "author": "dangreen",
  "license": "MIT",
  "keywords": [
    "gulp",
    "gulpplugin",
    "progressive-css",
    "progressive",
    "css"
  ],
  "dependencies": {
    "detect-indent": "^5.0.0",
    "html-minifier": "^3.3.1",
    "import-css": "^2.0.0",
    "mkdirp": "^0.5.1",
    "pn": "^1.0.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-trigen": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
